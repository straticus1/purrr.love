  "screenshots": [
    {
      "src": "/web/assets/screenshots/dashboard-wide.png",
      "sizes": "1280x800",
      "type": "image/png",
      "form_factor": "wide"
    },
    {
      "src": "/web/assets/screenshots/dashboard-narrow.png",
      "sizes": "375x812",
      "type": "image/png",
      "form_factor": "narrow"
    }
  ],
  "features": [
    "cross-origin-isolated"
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "share_target": {
    "action": "/web/api/share-photo.php",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "photo",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/web/photo-analysis.php",
      "accept": {
        "image/jpeg": [".jpg", ".jpeg"],
        "image/png": [".png"],
        "image/webp": [".webp"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+purrrlove",
      "url": "/web/handle-protocol.php?url=%s"
    }
  ]
}
