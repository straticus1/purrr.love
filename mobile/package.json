{
  "name": "purrr-mobile",
  "version": "1.0.0",
  "description": "Purrr.love Mobile App - Cat Gaming Ecosystem",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "xcodebuild -workspace ios/PurrrMobile.xcworkspace -scheme PurrrMobile -configuration Release -destination 'generic/platform=iOS' -archivePath ios/build/PurrrMobile.xcarchive archive",
    "clean": "react-native clean",
    "pods": "cd ios && pod install"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@react-native-async-storage/async-storage": "^1.19.2",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-firebase/messaging": "^18.3.0",
    "@react-native-firebase/analytics": "^18.3.0",
    "@react-native-camera-roll/camera-roll": "^5.7.1",
    "@react-native-picker/picker": "^2.4.10",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-camera": "^4.2.1",
    "react-native-device-info": "^10.8.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-image-picker": "^6.0.2",
    "react-native-keychain": "^8.1.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^3.4.2",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.22.1",
    "react-native-svg": "^13.10.0",
    "react-native-vector-icons": "^10.0.0",
    "react-redux": "^8.1.2",
    "redux-persist": "^6.0.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/runtime": "^7.22.6",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.17",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.1",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "metro-react-native-babel-preset": "^0.76.7",
    "prettier": "^3.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
